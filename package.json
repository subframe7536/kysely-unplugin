{
  "name": "kysely-unplugin-sqlite",
  "type": "module",
  "version": "0.1.0",
  "description": "Unplugin for Kysely and SQLite to optimize bundled size",
  "author": "subframe7536",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/kysely-unplugin.git"
  },
  "keywords": [
    "kysely",
    "sqlite",
    "database",
    "unplugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "format": "eslint --fix",
    "release": "bun example/build.ts && bun run build && bumpp --all && npm publish"
  },
  "peerDependencies": {
    "kysely": "^0.27.4",
    "magic-string": "^0.30.11",
    "unplugin": "^1.12.2"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.7.5",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^20.16.1",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "kysely": "^0.27.4",
    "kysely-wasm": "^0.6.3",
    "magic-string": "^0.30.11",
    "node-sqlite3-wasm": "^0.8.19",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
